<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="com.models.Booking" table="ticket" catalog="testdb">
		<composite-id>
			<key-property name="username" type="java.lang.String"/>
			<key-property name="sex" type="java.lang.String"/>
			<key-property name="begCity" type="java.lang.String"/>
			<key-property name="endCity" type="java.lang.String"/>
			<key-property name="date" type="java.lang.String"/>
			<key-property name="id" type="java.lang.String"/>
		</composite-id>
	</class>

</hibernate-mapping>