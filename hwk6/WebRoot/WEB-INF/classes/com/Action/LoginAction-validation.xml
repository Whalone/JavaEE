<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC 
  "-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
  "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
  
<validators>
  <field name="username">
    <field-validator type="requiredstring">
      <param name="trim">true</param>
      <message key="errUsername"></message>
    </field-validator>
  </field>
  
  <field name="password">
    <field-validator type="requiredstring">
      <param name="trim">true</param>
      <message key="errPassword"></message>
    </field-validator>
  </field>
  
  <field name="hobby">
    <field-validator type="requiredstring">
      <message key="errHobby"></message>
    </field-validator>
  </field>
</validators>